<h1>New Author</h1>

  <%= form_for :author do |f| %>
    <p>
      <%= f.label :'first name' %><br>
      <%= f.text_field :'first name' %>
    </p>
    <p>
      <%= f.label :'last name' %><br>
      <%= f.text_field :'last name' %>
    </p>
    <p>
      <%= f.label :'homepage' %><br>
      <%= f.text_field :'homepage' %>
    </p>
    <p>
      <%= f.submit %>
    </p>
  <% end %>


<p>Find me in app/views/authors/new.html.erb</p>
